{
  "name": "oorja",
  "description": "stream terminals to the web and more.",
  "version": "1.11.4",
  "keywords": [
    "teletype",
    "terminal",
    "sharing",
    "oorja",
    "cli"
  ],
  "homepage": "https://github.com/akshaykmr/teletype",
  "bugs": "https://github.com/akshaykmr/teletype/issues",
  "repository": "akshaykmr/teletype",
  "license": "SEE LICENSE IN license.txt",
  "author": "Akshay Kumar",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "oorja": "./bin/run.js",
    "teletype": "./bin/teletype.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "yarn build",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "oorja",
    "commands": "./dist/commands",
    "dirname": "oorja",
    "plugins": [
      "@oclif/plugin-help",
      "conf-cli"
    ]
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@oclif/core": "^3.26.4",
    "@oclif/plugin-help": "^6.0.21",
    "axios": "^1.6.8",
    "camelcase-keys": "^9.1.3",
    "chalk": "^5.3.0",
    "conf-cli": "0.1.9",
    "inquirer": "^9.2.20",
    "node-pty": "^1.0.0",
    "ora": "^8.0.1",
    "terminal-size": "^4.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^18",
    "eslint": "^8",
    "eslint-config-oclif": "^5.1.3",
    "eslint-config-oclif-typescript": "^3.1.6",
    "eslint-config-prettier": "^9.1.0",
    "oclif": "^4.9.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
