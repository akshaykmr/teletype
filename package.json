{
  "name": "oorja",
  "description": "stream terminals to the web and more.",
  "version": "1.9.0",
  "author": "Akshay Kumar @akshaykmr",
  "bin": {
    "oorja": "./bin/oorja",
    "teletype": "./bin/teletype"
  },
  "bugs": "https://github.com/akshaykmr/TeleType/issues",
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.4.0",
    "axios": "^1.5.0",
    "camelcase-keys": "^6.2.2",
    "chalk": "^4.1.2",
    "ora": "^4",
    "conf-cli": "^0.1.9",
    "enquirer": "^2.4.1",
    "node-pty": "^1.0.0",
    "term-size": "^2.2.0",
    "tslib": "^2.6.2",
    "ws": "^8.14.1"
  },
  "devDependencies": {
    "@types/node": "^16.18.50",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "oclif": "^3.15.0",
    "prettier": "^3.0.3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://oorja.io",
  "keywords": [
    "teletype",
    "terminal",
    "sharing",
    "oorja",
    "cli"
  ],
  "license": "SEE LICENSE IN license.txt",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "oorja",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "conf-cli"
    ]
  },
  "repository": "akshaykmr/TeleType",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md",
    "prettier": "prettier --write './**/*.{ts,tsx,js,css,html}'"
  },
  "types": "dist/index.d.ts",
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
